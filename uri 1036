#include <iostream>
#include<cmath>
#include<iomanip>
using namespace std;


int main() {
float A,B,C,R1,R2;
cin>>A;cin>>B;cin>>C;
if(A==0 || ((B*B)-(4*A*C))<0)
    {
    cout<<"Impossivel calcular\n";
}
else{
    R1=(-B+sqrt((B*B)-(4*A*C)))/(2*A);R2=(-B-sqrt((B*B)-(4*A*C)))/(2*A);

    cout<< "R1 = "<<fixed<<std::setprecision(5)<<R1<<endl;cout<< "R2 = "<<fixed<<std::setprecision(5)<<R2<<endl;

}

    return 0;

}
